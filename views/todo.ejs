<html>
<head>
  <title>Todo List </title>
  <link rel="shortcut icon" type='image/x-icon' href='/assets/favicon.ico'/>
  <script
  src="https://code.jquery.com/jquery-3.1.1.min.js" integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>
  <script src="/assets/todo-list.js"></script>
  <link href = "/assets/styles.css" rel = "stylesheet" type = "text/css" />
</head>
<body>
  <h1>My Todo List</h1>
  <div id = "todo-table">
    <form method="post">
      <input type="text" name="item" placeholder="Add new item..." required="true"/>
      <button type="submit">Add Item</button>
    </form>
    <ul>
      <% for(var i = 0;i<todos.length; i++) { %>
        <li><%= todos[i].item %></li>
      <% } %>
    </ul>
  </div>
</body>
</html>
